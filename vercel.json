{
  "version": 2,
  "builds": [
    {
      "src": "backend/dist/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/welcome.html"
    },
    {
      "src": "/api/users/login",
      "dest": "/login.html"
    },
    {
      "src": "/api/users/signup",
      "dest": "/signup.html"
    },
    {
      "src": "/main",
      "dest": "/main.html"
    }
  ]
}